page {
  background-color: #fff;
}
.btn_reset {
  width: 140rpx;
  height: 50rpx;
  line-height: 50rpx;
  background-color: #fff;
  border-radius: 24rpx;
  text-align: center;
  font-size: 24rpx;
  color: #666;
}
/* 下拉菜单 */

/* 下拉菜单导航 */

.menu-po {
  width: 100%;
  position: fixed;
  /* top:100rpx; */
  background: #fff;
  z-index: 9999;

}

.menu {
  background-color: #fff;
  border-bottom: 1rpx solid #e9e9e9;
}

.menu-list {
  display: flex;
  justify-content: space-between;
  height: 90rpx;
  padding: 0 100rpx;
}

.menu-nav {
  width: 25%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.menu-nav image {
  width: 32rpx;
  height: 32rpx;
}

.menu-if {
  background-color: #fff;
  box-sizing: border-box;
  position: absolute;
  top: 92rpx;
  left: 0;
  z-index: 9;
  width: 100%;
}

.meun-txt {
  height: auto;
  color: #696969;
}

.menu-text-list {
  position: relative;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
}
.menu-text-list-left {
  /* width: 100%; */
  background-color: #fff;
  box-sizing: border-box;
  padding-left: 35rpx;
}

/* 二级样式 */

.meun-item {
  background: #f8f8f8;
  width: 150rpx;
  height: 55rpx;
  line-height: 55rpx;
  margin-left: 30rpx;
  margin-bottom: 30rpx;
  text-align: center;
  float: left;
}


.menu-text-list-rig {
  width: 50%;
  position: absolute;
  top: 0;
  right: 0;
  background-color: #f9f9f9;
  z-index: 10;
  box-sizing: border-box;
  padding-left: 35rpx;
}

/* over/auto */

.over-auto {
  background: #fff;
  padding-top: 30rpx;
  padding-right: 30rpx;
  overflow: auto;
}
.selected{
  color: #f89521;
  font-weight: bold;
}

.cars-item{
  padding: 0 30rpx 80rpx 30rpx;
}
.cars image{
  width: 219rpx;
  height: 109rpx;
}
.money{
  color: rgb(245,51,19);
}